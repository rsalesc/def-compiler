def int mod(int x, int MOD){
  int q = x/MOD;
  return x-q*MOD;
}

def int gcd(int a, int b){
  if(b == 0){
    return a;
  }

  return gcd(b, mod(a,b));
}

def int main(){
  print(gcd(42, 21));
  print(gcd(259912, 1902931));
  print(gcd(1, 0));
  print(gcd(42, 42));
  print(gcd(0, 0));
  print(gcd(42, 56));
}
