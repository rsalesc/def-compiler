def int mod(int x, int MOD){
  int q = x/MOD;
  return x-q*MOD;
}

def int count(int n){
  int i;

  if(n == 1){
    return 0;
  }

  i = 2;
  while(i < n && mod(n, i)){
    i = i+1;
  }

  while(mod(n, i) == 0){
    n = n/i;
  }

  return 1+count(n);
}

def void solve(int x){
  print(count(x));
}

def void main(){
  solve(42);
