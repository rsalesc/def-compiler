def int mod(int x, int MOD){
  int q = x/MOD;
  return x - q*MOD;
}

def int phi(int n){
  int res = n;
  int i = 2;
  while(i*i <= n){
    if(mod(n, i) == 0){
      while(mod(n, i) == 0){
        n = n/i;
      }
      res = res - res/i;
    }

    i = i+1;
  }

  if(n > 1){
    res = res - res/n;
  }

  return res;
}

def void solve(int x){
  print(phi(x));
}

def void main(){
  solve(42);
  solve(56);
  solve(20910921);
  solve(1717171);
  solve(97);
  solve(1024);
}
