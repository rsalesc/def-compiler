int MOD;

// mod for positive numbers
def int mod(int x){
  int q = x/MOD;
  return x-q*MOD;
}

def int f(int f){
  if(f == 0) {return 0;}
  if(f == 1) {return mod(1);}
  return mod(f(f-1)+f(f-2));
}

def void solve(int a, int b){
  MOD = b;
  print(f(a));
}

def void main(){
  int i;
  solve(1, 1);
  solve(10, 10);
  solve(17, 21);
  solve(15, 1000000);

  while(i <= 10){
    solve(i, 10000);
    i = i+1;
  }
}
